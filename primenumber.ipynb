   "source": [
    "num=int(input())\n",
    "if num>1: #check the num greater thn 1\n",
    "    for i in range(2,num): \n",
    "        if (num % i) == 0:\n",
    "            print(\"not a prime number\")\n",
    "            break\n",
    "    else:\n",
    "        print(\"prime\") \n",
    "else:\n",
    "    print('not a prime')"
   ]
